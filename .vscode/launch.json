{
    "version": "0.2.0",
    "configurations": [
        {
        "type": "chrome",
        "request": "launch",
        "name": "Launch Chrome",
        "url": "http://localhost:9000",
        "webRoot": "${workspaceFolder}/src",
        "sourceMaps": true,
        "trace": true,
        "outFiles": ["${workspaceFolder}/dist/**/*.js"],
        "sourceMapPathOverrides": {
            "webpack:///src/*": "${workspaceFolder}/src/*",
            "webpack:///*": "*",
            "webpack:///./*": "${workspaceFolder}/*"
        }
        }
    ]
    }